<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="my_test.options_provider.config.class">Starternh\MyTestBundle\Options\ConfigProvider</parameter>
    </parameters>

    <services>
        <service id="my_test.options_provider.config" class="%my_test.options_provider.config.class%">
            <argument>%my_test.map%</argument>
            <argument>%my_test.defaults%</argument>
            <tag name="my_test.options_provider" priority="-1" />
        </service>
    </services>
</container>
